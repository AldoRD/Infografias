      targets: item,
      translateX: [`+=${diff.x * 3 + offset}`, 0],
      translateY: [`+=${diff.y * 3 + offset}`, 0],
      rotate: [20, 0],
      easing: easingAnim,
      duration: durationAnim,
    })
  })
